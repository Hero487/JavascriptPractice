//q6
class Wizard {
		 health;
		 mana;
		 armor;
	attack(){
			console.log("파이어볼");
		}
	constructor(a, b, c){
		this.health = a;
		this.mana = b;
		this.armor = c;
	}
	}
const x = new Wizard(545, 210, 10);
console.log(x.health, x.mana, x.armor);
x.attack();

