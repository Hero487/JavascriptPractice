//q3 
var year = '2019';
var month = '04';
var day = '26';
var hour = '11';
var minute = '34';
var second = '27';

var result = year +"/" + month +"/"+day +" "+hour +":"+minute +":"+second;
alert(result);
console.log(result);

//221020
//q4
var star = '';
for(var i = 0; i < 5; i++){
  for(var j =  4; j >= 0; j--){
	  if(j <= 4)
	    if(i >= j){
	    	star += "*";
	    }else{
	    	star += " ";
	 	 }
	  else{

	  }
	}
  for(var j = 0; j < 5; j++){
		if(i > j){
			star += "*";
		}
		else{
			star += " ";
		}
	}
  star += "\n";
}
console.log(star);

